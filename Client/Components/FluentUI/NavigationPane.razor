@inject IJSRuntime JSRuntime;

<div id="@Name" />

@code {
	[Parameter]
	public string? Name { get; set; } = null!;

	[Parameter]
	public string? Title { get; set; } = null!;

	[Parameter]
	public Dictionary<string, object>? NavigationItems { get; set; } = null!;

	protected override void OnInitialized()
	{

	}

	protected override async Task OnAfterRenderAsync(bool firstRender)
	{
		if (firstRender)
		{
			await JSRuntime.InvokeVoidAsync("JsFunctions.renderNavigationPane", Title, NavigationItems, Name);
		}
	}
}