{
	"compileOnSave": true,
	"compilerOptions": {
		"jsx": "react",
		"target": "ES2015",
		"sourceMap": true,
		"noEmitOnError": true,
		"noImplicitAny": false,
		"outDir": "wwwroot/js",
		"removeComments": false,
		"moduleResolution": "node",
		"preserveConstEnums": true,
		"experimentalDecorators": true
	}
}